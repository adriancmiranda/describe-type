{
  "name": "describe-type",
  "author": "Adrian C. Miranda",
  "description": "Evaluates whether an object is compatible with a specific data type.",
  "version": "1.0.0",
  "types": "index.d.ts",
  "main": "index.js",
  "module": "index.next.js",
  "unpkg": "dist/describe-type.umd.js",
  "homepage": "https://github.com/adriancmiranda/describe-type",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "as/**/!(*.unit|*.perf).js",
    "has/**/!(*.unit|*.perf).js",
    "internal/**/!(*.unit|*.perf).js",
    "is/**/!(*.unit|*.perf).js",
    "polyfill/**/!(*.unit|*.perf).js",
    "schema/**/!(*.unit|*.perf).js",
    "index.d.ts",
    "index.next.js",
    "index.js"
  ],
  "scripts": {
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "flow": "flow",
    "lint": "eslint --ext .js as,has,internal,is,polyfill,schema",
    "prereport-coverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "report-coverage": "codecov -f coverage.lcov --token=CODECOV_TOKEN",
    "test:fixture": "cross-env NODE_ENV=test node .bin/fixture",
    "test:perf": "cross-env NODE_ENV=test node .bin/benchmark",
    "test:appveyor": "npm test -- --verbose",
    "test:travis": "npm test -- --verbose",
    "test:unit": "cross-env NODE_ENV=test node .bin/ava --color --no-cache --watch",
    "test": "npm run flow && npm run test:unit -- --watch=false **/*.unit.js",
    "dev": "cross-env NODE_ENV=test npm run test:unit",
    "rollup": "rollup -c .bin/index.js",
    "rollup:watch": "npm run rollup -- -w --environment=NODE_ENV:development",
    "unbuild": "rimraf dist/*.{js,map,gz}",
    "build:doc": "documentation build index.next.js -f html -o docs",
    "build:gzip": "npm run rollup -- --environment=SIGN,MINIFY,GZIP:js,NODE_ENV:production",
    "build:minify": "npm run rollup -- --environment=SIGN,MINIFY,NODE_ENV:production",
    "build:concat": "npm run unbuild && npm run rollup -- --environment=SIGN,INDENT,NODE_ENV:production",
    "build:cjs:internal": "node .bin/babel internal/{*,**/*}",
    "build:cjs:as": "node .bin/babel as/{*,**/*}.next.js",
    "build:cjs:has": "node .bin/babel has/{*,**/*}.next.js",
    "build:cjs:is": "node .bin/babel is/{*,**/*}.next.js",
    "build:cjs:polyfill": "node .bin/babel polyfill/{*,**/*}.next.js",
    "build:cjs:schema": "node .bin/babel schema/{*,**/*}.next.js",
    "build:cjs": "node .bin/babel index.next.js {internal,as,has,is,polyfill,schema}/{*,**/*}.next.js",
    "build": "redrun -s build:cjs build:concat build:minify build:gzip",
    "analyze:type:amd": "source-map-explorer dist/describe-type.amd.min.js.map",
    "analyze:type:iife": "source-map-explorer dist/describe-type.iife.min.js.map",
    "analyze:type:umd": "source-map-explorer dist/describe-type.umd.min.js.map",
    "analyze:as:amd": "source-map-explorer dist/describe-type.as.amd.min.js.map",
    "analyze:as:iife": "source-map-explorer dist/describe-type.as.iife.min.js.map",
    "analyze:as:umd": "source-map-explorer dist/describe-type.as.umd.min.js.map",
    "analyze:has:amd": "source-map-explorer dist/describe-type.has.amd.min.js.map",
    "analyze:has:iife": "source-map-explorer dist/describe-type.has.iife.min.js.map",
    "analyze:has:umd": "source-map-explorer dist/describe-type.has.umd.min.js.map",
    "analyze:is:amd": "source-map-explorer dist/describe-type.is.amd.min.js.map",
    "analyze:is:iife": "source-map-explorer dist/describe-type.is.iife.min.js.map",
    "analyze:is:umd": "source-map-explorer dist/describe-type.is.umd.min.js.map",
    "analyze:internal:amd": "source-map-explorer dist/describe-type.internal.amd.min.js.map",
    "analyze:internal:iife": "source-map-explorer dist/describe-type.internal.iife.min.js.map",
    "analyze:internal:umd": "source-map-explorer dist/describe-type.internal.umd.min.js.map",
    "precommit": "npm run test",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-cli": "6.26.0",
    "babel-plugin-transform-es2015-modules": "github:adriancmiranda/babel-plugin-transform-es2015-modules",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "benchmark": "2.1.4",
    "browser-env": "3.2.5",
    "chokidar": "2.0.3",
    "codecov": "3.0.1",
    "colors": "1.2.3",
    "conventional-commit-types": "2.2.0",
    "cross-env": "5.1.4",
    "documentation": "6.3.2",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "flow-bin": "0.71.0",
    "git-revision-webpack-plugin": "3.0.1",
    "glob": "7.1.2",
    "husky": "0.14.3",
    "make-arrow-function": "1.1.0",
    "make-generator-function": "1.1.0",
    "microtime": "2.1.7",
    "moment": "2.22.1",
    "npm-check-updates": "2.14.2",
    "nyc": "11.7.1",
    "pirate-flag": "0.1.0",
    "redrun": "6.0.4",
    "rimraf": "2.6.2",
    "rollup": "0.58.2",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-buble": "0.19.2",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-es3": "1.1.0",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-gzip": "1.3.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-optimize-js": "0.0.4",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "3.0.0",
    "rollup-watch": "4.3.1",
    "source-map-explorer": "1.5.0",
    "standard-version": "4.3.0",
    "strip-json-comments": "2.0.1",
    "tempy": "0.2.1",
    "tosource": "1.0.0",
    "uglify-es": "3.3.9",
    "validate-commit-msg": "2.14.0"
  },
  "ava": {
    "tap": false,
    "failFast": true,
    "powerAssert": false,
    "failWithoutAssertions": false,
    "concurrency": 5,
    "require": [
      "babel-polyfill",
      "babel-register",
      "./.bin/ava/setup-browser-env.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/describe-type.git"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "keywords": [
    "describe-type",
    "utility"
  ]
}

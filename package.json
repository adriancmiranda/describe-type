{
  "name": "describe-type",
  "author": "Adrian C. Miranda",
  "description": "Evaluates whether an object is compatible with a specific data type.",
  "version": "0.0.1",
  "types": "index.d.ts",
  "main": "index.js",
  "browser": "index.js",
  "module": "index",
  "homepage": "https://github.com/adriancmiranda/describe-type",
  "readmeFilename": "README.md",
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "lib",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "report-coverage": "codecov -f test/coverage/coverage-final.json",
    "doc": "documentation build index.js -f html -o docs",
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "lint": "eslint --ext .js lib",
    "test:unit": "run-s \"test:watch -- --single-run\"",
    "test:watch": "ava --no-cached --verbose",
    "test": "run-s test:unit",
    "precommit": "run-s test",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version"
  },
  "devDependencies": {
    "ava": "~0.19.1",
    "codecov": "~2.2.0",
    "conventional-commit-types": "~2.1.0",
    "documentation": "~4.0.0-rc.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "1.16.0",
    "husky": "~0.14.0",
    "npm-check-updates": "~2.12.0",
    "npm-run-all": "~4.0.2",
    "standard-version": "~4.2.0",
    "validate-commit-msg": "~2.12.2"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "engines": {
    "node": ">=6.10.1"
  },
  "keywords": [
    "describe-type",
    "describe",
    "type",
    "typeof",
    "typeOf",
    "constructorNameOf",
    "constructorOf",
    "typify",
    "as",
    "is"
  ]
}

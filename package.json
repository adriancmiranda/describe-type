{
  "name": "describe-type",
  "author": "Adrian C. Miranda",
  "description": "Evaluates whether an object is compatible with a specific data type.",
  "version": "0.3.0",
  "types": "index.d.ts",
  "main": "index.js",
  "browser": "index.js",
  "module": "index",
  "homepage": "https://github.com/adriancmiranda/describe-type",
  "readmeFilename": "README.md",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "lib",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "prereport:coverage": "nyc report --reporter=text-lcov npm test > test/coverage.lcov",
    "report:coverage": "codecov -f test/coverage.lcov --token=$CODECOV_TOKEN",
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "lint": "eslint --ext .js index.js lib",
    "dev": "run-p watch:*",
    "test": "mocha test/**/*.unit.js -G -c -b --inline-diffs",
    "test:speed": "node test/speed",
    "test:coverage": "nyc npm test",
    "test:travis": "nyc npm test",
    "test:appveyor": "nyc mocha test\\**\\*.unit.js -G -c -b --inline-diffs",
    "build": "run-p build:*",
    "build:min": "run-p \"build:modules -- --environment=min:true\"",
    "build:modules": "rollup -c ./rollup.config.js",
    "watch:test": "npm test -- -w",
    "precommit": "run-s test",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version",
    "uninstall": "rimraf -rf node_modules"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.1.0",
    "codecov": "^2.2.0",
    "conventional-commit-types": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^1.16.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "husky": "^0.14.2",
    "mocha": "^3.4.2",
    "moment": "^2.18.1",
    "npm-check-updates": "^2.12.1",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.3",
    "pirate-flag": "^0.1.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-optimize-js": "^0.0.4",
    "rollup-plugin-uglify": "^2.0.1",
    "standard-version": "^4.2.0",
    "uglify": "^0.1.5",
    "uglify-es": "^3.0.22",
    "validate-commit-msg": "^2.12.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/describe-type.git"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "keywords": [
    "describe-type",
    "is",
    "util"
  ]
}

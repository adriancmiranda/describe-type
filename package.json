{
  "name": "describe-type",
  "author": "Adrian C. Miranda",
  "description": "Evaluates whether an object is compatible with a specific data type.",
  "version": "0.3.0",
  "types": "index.d.ts",
  "main": "index.js",
  "browser": "index.js",
  "module": "index",
  "homepage": "https://github.com/adriancmiranda/describe-type",
  "readmeFilename": "README.md",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "lib",
    "index.d.ts",
    "index.js"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "bin": "rollup -c bin/index.js",
    "analyze:type:amd": "source-map-explorer dist/type.amd.min.js.map",
    "analyze:type:iife": "source-map-explorer dist/type.iife.min.js.map",
    "analyze:type:umd": "source-map-explorer dist/type.umd.min.js.map",
    "analyze:as:amd": "source-map-explorer dist/type.as.amd.min.js.map",
    "analyze:as:iife": "source-map-explorer dist/type.as.iife.min.js.map",
    "analyze:as:umd": "source-map-explorer dist/type.as.umd.min.js.map",
    "analyze:constructorNameOf:amd": "source-map-explorer dist/type.constructorNameOf.amd.min.js.map",
    "analyze:constructorNameOf:iife": "source-map-explorer dist/type.constructorNameOf.iife.min.js.map",
    "analyze:constructorNameOf:umd": "source-map-explorer dist/type.constructorNameOf.umd.min.js.map",
    "analyze:constructorOf:amd": "source-map-explorer dist/type.constructorOf.amd.min.js.map",
    "analyze:constructorOf:iife": "source-map-explorer dist/type.constructorOf.iife.min.js.map",
    "analyze:constructorOf:umd": "source-map-explorer dist/type.constructorOf.umd.min.js.map",
    "analyze:is:amd": "source-map-explorer dist/type.is.amd.min.js.map",
    "analyze:is:iife": "source-map-explorer dist/type.is.iife.min.js.map",
    "analyze:is:umd": "source-map-explorer dist/type.is.umd.min.js.map",
    "analyze:name:amd": "source-map-explorer dist/type.name.amd.min.js.map",
    "analyze:name:iife": "source-map-explorer dist/type.name.iife.min.js.map",
    "analyze:name:umd": "source-map-explorer dist/type.name.umd.min.js.map",
    "analyze:of:amd": "source-map-explorer dist/type.of.amd.min.js.map",
    "analyze:of:iife": "source-map-explorer dist/type.of.iife.min.js.map",
    "analyze:of:umd": "source-map-explorer dist/type.of.umd.min.js.map",
    "analyze:to:amd": "source-map-explorer dist/type.to.amd.min.js.map",
    "analyze:to:iife": "source-map-explorer dist/type.to.iife.min.js.map",
    "analyze:to:umd": "source-map-explorer dist/type.to.umd.min.js.map",
    "analyze:typify:amd": "source-map-explorer dist/type.typify.amd.min.js.map",
    "analyze:typify:iife": "source-map-explorer dist/type.typify.iife.min.js.map",
    "analyze:typify:umd": "source-map-explorer dist/type.typify.umd.min.js.map",
    "report:coverage": "npm run test:cover && codecov -f test/coverage.lcov --token=$CODECOV_TOKEN",
    "build:doc": "documentation build index.js -f html -o docs",
    "build:gzip": "npm run bin -- --environment=SIGN,MINIFY,GZIP:js,NODE_ENV:production",
    "build:minify": "npm run bin -- --environment=SIGN,MINIFY,NODE_ENV:production",
    "build:concat": "npm run unbuild && npm run bin -- --environment=SIGN,INDENT,NODE_ENV:production",
    "build:example": "cd example && node",
    "build": "redrun -p build:concat build:minify build:gzip",
    "test:sauce": "karma start bin/karma/sauce.config.js",
    "test:travis": "karma start bin/karma/ci.config.js",
    "test:appveyor": "karma start bin/karma/ci.config.js",
    "test:cover": "cross-env NODE_ENV=test karma start bin/karma/cover.config.js",
    "test:unit": "cross-env NODE_ENV=test karma start bin/karma/unit.config.js",
    "test:speed": "node bin/benchmark",
    "test": "redrun -s lint test:unit",
    "dev": "cross-env NODE_ENV=test karma start bin/karma/dev.config.js",
    "watch": "npm run bin -- -w --environment=NODE_ENV:development",
    "lint": "eslint --ext .js index.js lib",
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "precommit": "npm run test",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version",
    "untest": "rimraf *.log test/unit/coverage",
    "unbuild": "rimraf dist/*.{js,map,gz}",
    "uninstall": "rimraf -rf node_modules package-lock.json"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "benchmark": "^2.1.4",
    "chokidar": "^1.7.0",
    "codecov": "^2.3.0",
    "conventional-commit-types": "^2.2.0",
    "cross-env": "^5.0.4",
    "documentation": "^5.1.1",
    "eslint": "^4.4.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "jasmine-core": "^2.7.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-failed-reporter": "^0.0.3",
    "karma-firefox-launcher": "^1.0.1",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-jasmine-matchers": "^3.7.0",
    "karma-json-fixtures-preprocessor": "^0.0.6",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.4",
    "moment": "^2.18.1",
    "npm-check-updates": "^2.12.1",
    "pirate-flag": "^0.1.0",
    "redrun": "^5.9.15",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "8.1.0",
    "rollup-plugin-gzip": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-optimize-js": "^0.0.4",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "sinon": "^3.0.0",
    "source-map-explorer": "^1.4.0",
    "standard-version": "^4.2.0",
    "uglify-es": "3.0.27",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/describe-type.git"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "keywords": [
    "describe-type",
    "utility"
  ]
}

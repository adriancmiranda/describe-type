{
  "name": "describe-type",
  "author": "Adrian C. Miranda",
  "description": "Evaluates whether an object is compatible with a specific data type.",
  "version": "0.2.0",
  "types": "index.d.ts",
  "main": "index.js",
  "browser": "index.js",
  "module": "index",
  "homepage": "https://github.com/adriancmiranda/describe-type",
  "readmeFilename": "README.md",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "lib",
    "index.d.ts",
    "index.js",
    "bower.json"
  ],
  "scripts": {
    "prereport-coverage": "nyc report --reporter=text-lcov > test/coverage.lcov",
    "report-coverage": "codecov -f test/coverage.lcov --token=CODECOV_TOKEN",
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "lint": "eslint --ext .js index.js lib",
    "dev": "run-p watch:*",
    "test": "ava **/*.test.js --color",
    "test:coverage": "nyc npm test -- --no-cache --verbose",
    "test:travis": "nyc npm test -- --no-cache --verbose",
    "test:appveyor": "nyc npm test -- --no-cache --verbose",
    "build": "run-p build:*",
    "build:min": "run-p \"build:modules -- --environment=min:true\"",
    "build:modules": "rollup -c ./rollup.config.js",
    "watch:test": "npm test -- --watch",
    "precommit": "run-s test",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "browser-env": "^3.0.2",
    "codecov": "^2.2.0",
    "conventional-commit-types": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^1.16.0",
    "git-revision-webpack-plugin": "^2.5.1",
    "husky": "^0.14.2",
    "moment": "^2.18.1",
    "npm-check-updates": "^2.12.1",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.3",
    "pirate-flag": "^0.1.0",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-optimize-js": "^0.0.4",
    "rollup-plugin-uglify": "^2.0.1",
    "standard-version": "^4.2.0",
    "uglify": "^0.1.5",
    "uglify-es": "^3.0.22",
    "validate-commit-msg": "^2.12.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/describe-type.git"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "ava": {
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "keywords": [
    "describe-type",
    "describe",
    "type",
    "typeof",
    "typeOf",
    "constructorNameOf",
    "constructorOf",
    "typify",
    "test",
    "util",
    "as",
    "is"
  ]
}

{
  "name": "describe-type",
  "author": "Adrian C. Miranda",
  "description": "Evaluates whether an object is compatible with a specific data type.",
  "version": "0.7.0",
  "types": "./source/index.d.ts",
  "main": "./dist/type.cjs.js",
  "module": "./source/index.js",
  "homepage": "https://github.com/adriancmiranda/describe-type",
  "license": "MIT",
  "directories": {
    "lib": "source",
    "test": "test"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "source"
  ],
  "scripts": {
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "flow": "flow",
    "lint": "eslint --ext .js source",
    "prereport-coverage": "nyc report --reporter=text-lcov > test/coverage.lcov",
    "report-coverage": "codecov -f test/coverage.lcov --token=CODECOV_TOKEN",
    "test:fixture": "cross-env NODE_ENV=test node bin/fixture",
    "test:speed": "cross-env NODE_ENV=test node bin/benchmark",
    "test:appveyor": "npm test -- --verbose",
    "test:travis": "npm test -- --verbose",
    "test:unit": "cross-env NODE_ENV=test node bin/ava --color --no-cache --watch",
    "test": "npm run flow && npm run test:unit -- --watch=false **/*.unit.js",
    "dev": "cross-env NODE_ENV=test npm run test:unit",
    "watch": "npm run bin -- -w --environment=NODE_ENV:development",
    "bin": "rollup -c bin/index.js",
    "unbuild": "rimraf dist/*.{js,map,gz}",
    "build:doc": "documentation build index.js -f html -o docs",
    "build:gzip": "npm run bin -- --environment=SIGN,MINIFY,GZIP:js,NODE_ENV:production",
    "build:minify": "npm run bin -- --environment=SIGN,MINIFY,NODE_ENV:production",
    "build:concat": "npm run unbuild && npm run bin -- --environment=SIGN,INDENT,NODE_ENV:production",
    "build": "redrun -p build:concat build:minify build:gzip",
    "analyze:type:amd": "source-map-explorer dist/type.amd.min.js.map",
    "analyze:type:iife": "source-map-explorer dist/type.iife.min.js.map",
    "analyze:type:umd": "source-map-explorer dist/type.umd.min.js.map",
    "analyze:as:amd": "source-map-explorer dist/type.as.amd.min.js.map",
    "analyze:as:iife": "source-map-explorer dist/type.as.iife.min.js.map",
    "analyze:as:umd": "source-map-explorer dist/type.as.umd.min.js.map",
    "analyze:builtin:amd": "source-map-explorer dist/type.built-in.amd.min.js.map",
    "analyze:builtin:iife": "source-map-explorer dist/type.built-in.iife.min.js.map",
    "analyze:builtin:umd": "source-map-explorer dist/type.built-in.umd.min.js.map",
    "analyze:has:amd": "source-map-explorer dist/type.has.amd.min.js.map",
    "analyze:has:iife": "source-map-explorer dist/type.has.iife.min.js.map",
    "analyze:has:umd": "source-map-explorer dist/type.has.umd.min.js.map",
    "analyze:is:amd": "source-map-explorer dist/type.is.amd.min.js.map",
    "analyze:is:iife": "source-map-explorer dist/type.is.iife.min.js.map",
    "analyze:is:umd": "source-map-explorer dist/type.is.umd.min.js.map",
    "analyze:@:amd": "source-map-explorer dist/type.@.amd.min.js.map",
    "analyze:@:iife": "source-map-explorer dist/type.@.iife.min.js.map",
    "analyze:@:umd": "source-map-explorer dist/type.@.umd.min.js.map",
    "precommit": "npm run test",
    "commitmsg": "validate-commit-msg",
    "release": "standard-version"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "ava-spec": "1.1.1",
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "benchmark": "2.1.4",
    "browser-env": "3.2.5",
    "chokidar": "2.0.3",
    "codecov": "3.0.1",
    "colors": "^1.2.1",
    "conventional-commit-types": "2.2.0",
    "cross-env": "5.1.4",
    "documentation": "6.3.2",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "flow-bin": "0.71.0",
    "git-revision-webpack-plugin": "3.0.1",
    "glob": "7.1.2",
    "husky": "0.14.3",
    "make-arrow-function": "1.1.0",
    "make-generator-function": "1.1.0",
    "microtime": "^2.1.7",
    "moment": "2.22.1",
    "npm-check-updates": "2.14.2",
    "nyc": "11.7.1",
    "pirate-flag": "0.1.0",
    "redrun": "6.0.4",
    "rimraf": "2.6.2",
    "rollup": "0.58.2",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-buble": "0.19.2",
    "rollup-plugin-commonjs": "9.1.2",
    "rollup-plugin-es3": "1.1.0",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-gzip": "1.3.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-optimize-js": "0.0.4",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-uglify": "3.0.0",
    "rollup-watch": "4.3.1",
    "source-map-explorer": "1.5.0",
    "standard-version": "4.3.0",
    "strip-json-comments": "2.0.1",
    "tempy": "0.2.1",
    "tosource": "1.0.0",
    "uglify-es": "3.3.9",
    "validate-commit-msg": "2.14.0"
  },
  "ava": {
    "tap": false,
    "failFast": true,
    "powerAssert": false,
    "failWithoutAssertions": false,
    "concurrency": 5,
    "require": [
      "./test/helpers/setup-next-support.js",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/describe-type.git"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "keywords": [
    "describe-type",
    "utility"
  ]
}

{
  "name": "describe-type",
  "author": "Adrian C. Miranda",
  "description": "Evaluates whether an object is compatible with a specific data type.",
  "version": "1.0.0",
  "types": "index.d.ts",
  "main": "index.js",
  "module": "index.next.js",
  "unpkg": "dist/describe-type.umd.js",
  "homepage": "https://github.com/adriancmiranda/describe-type",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "as/**/!(*.unit|*.perf).js",
    "has/**/!(*.unit|*.perf).js",
    "internal/**/!(*.unit|*.perf).js",
    "is/**/!(*.unit|*.perf).js",
    "polyfill/**/!(*.unit|*.perf).js",
    "schema/**/!(*.unit|*.perf).js",
    "index.d.ts",
    "index.next.js",
    "index.js"
  ],
  "scripts": {
    "upgrade": "npx npm-check -uEy",
    "flow": "flow",
    "lint": "eslint --ext .js as,has,internal,is,polyfill,schema",
    "prereport-coverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "report-coverage": "codecov -f coverage.lcov --token=CODECOV_TOKEN",
    "test:fixture": "cross-env NODE_ENV=test node .bin/fixture",
    "test:perf": "cross-env NODE_ENV=test node .bin/benchmark",
    "test:appveyor": "npm test -- --verbose",
    "test:travis": "npm test -- --verbose",
    "test:unit": "cross-env NODE_ENV=test ava --color --no-cache",
    "test": "npm run test:unit -- {*,**/*}.unit.js",
    "dev": "cross-env NODE_ENV=test npm run test -- --watch",
    "rollup": "rollup -c .bin/index.js",
    "rollup:watch": "npm run rollup -- -w --environment=NODE_ENV:development",
    "unbuild": "rimraf dist/*.{js,map,gz}",
    "build:doc": "documentation build index.next.js -f html -o docs",
    "build:gzip": "npm run rollup -- --environment=SIGN,MINIFY,GZIP:js,NODE_ENV:production",
    "build:minify": "npm run rollup -- --environment=SIGN,MINIFY,NODE_ENV:production",
    "build:concat": "npm run unbuild && npm run rollup -- --environment=SIGN,INDENT,NODE_ENV:production",
    "build:cjs:internal": "unkn internal/{*,**/*} --replace='/.next(.js)?/i,$1'",
    "build:cjs:as": "unkn as/{*,**/*}.next.js --replace='/.next(.js)?/i,$1'",
    "build:cjs:has": "unkn has/{*,**/*}.next.js --replace='/.next(.js)?/i,$1'",
    "build:cjs:is": "unkn is/{*,**/*}.next.js --replace='/.next(.js)?/i,$1'",
    "build:cjs:polyfill": "unkn polyfill/{*,**/*}.next.js --replace='/.next(.js)?/i,$1'",
    "build:cjs:schema": "unkn schema/{*,**/*}.next.js --replace='/.next(.js)?/i,$1'",
    "build:cjs": "unkn index.next.js {internal,as,has,is,polyfill,schema}/{*,**/*}.next.js --replace='/.next(.js)?/i,$1'",
    "build": "redrun -s build:cjs build:concat build:minify build:gzip",
    "precommit": "npm run lint && npm run test",
    "commitmsg": "commitlint -x @commitlint/config-conventional -e $GIT_PARAMS",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/node": "7.0.0-beta.44",
    "@babel/plugin-external-helpers": "7.0.0-beta.44",
    "@babel/plugin-transform-spread": "7.0.0-beta.44",
    "@babel/polyfill": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/register": "7.0.0-beta.44",
    "@commitlint/cli": "6.2.0",
    "@commitlint/config-conventional": "6.1.3",
    "ava": "1.0.0-beta.4",
    "babel-eslint": "8.2.3",
    "benchmark": "2.1.4",
    "browser-env": "3.2.5",
    "chokidar": "2.0.3",
    "codecov": "3.0.2",
    "colors": "1.3.0",
    "cross-env": "5.1.6",
    "cross-spawn": "6.0.5",
    "documentation": "7.1.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.8.2",
    "flow-bin": "0.73.0",
    "git-revision-webpack-plugin": "3.0.3",
    "glob": "7.1.2",
    "husky": "0.14.3",
    "make-arrow-function": "1.1.0",
    "make-generator-function": "1.1.0",
    "microtime": "2.1.8",
    "moment": "2.22.1",
    "nyc": "11.8.0",
    "pirate-flag": "0.1.0",
    "read-argv": "1.0.0",
    "redrun": "6.0.4",
    "rimraf": "2.6.2",
    "rollup": "0.59.3",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-babel": "4.0.0-beta.4",
    "rollup-plugin-buble": "0.19.2",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-es3": "1.1.0",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-optimize-js": "0.0.4",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-terser": "1.0.1",
    "rollup-watch": "4.3.1",
    "safe-regex": "1.1.0",
    "standard-version": "4.4.0",
    "strip-json-comments": "2.0.1",
    "tempy": "0.2.1",
    "tosource": "1.0.0",
    "uglify-es": "3.3.9",
    "unkn": "0.0.3"
  },
  "ava": {
    "tap": false,
    "failFast": false,
    "powerAssert": false,
    "failWithoutAssertions": false,
    "concurrency": 5,
    "require": [
      "@babel/polyfill",
      "./.bin/ava/babel-register.js",
      "./.bin/ava/browser-env.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/describe-type.git"
  },
  "keywords": [
    "describe-type",
    "utility"
  ]
}
